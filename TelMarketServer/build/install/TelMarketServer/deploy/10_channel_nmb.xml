<channel-adaptor name='nmb-channel' class="org.jpos.q2.iso.ChannelAdaptor"
		logger="Q2">
	<channel class="org.jpos.iso.channel.XMLChannel"
		packager="org.jpos.iso.packager.XML2003Packager">
	<property name="host" value="localhost" />
	<property name="port" value="7113" />
        <property name="timeout" value="30000" />
	</channel>
<in>nmb-send</in>
<out>nmb-receive</out>
<reconnect-delay>10000</reconnect-delay>
</channel-adaptor>
